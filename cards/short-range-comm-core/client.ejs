<div id="short-range-comm-Core-Widget_UserPrefsWindow" class="coreWindow" windowName="Comm Channels" style="width: 150px;height: 400px;">
    <canvas id="short-range-comm-Core-Widget_UserPrefsWindow_sliderCanvas"></canvas>
    <div id="short-range-comm-Core-Widget_UserPrefsWindow_label">Quick Hail Preset</div>
    <input type="button" value="+" class="coreButton" id="short-range-comm-Core-Widget_UserPrefsWindow_addButton">
    <input type="button" value="-" class="coreButton" id="short-range-comm-Core-Widget_UserPrefsWindow_removeButton">
    <select id="short-range-comm-Core-Widget_UserPrefsWindow_defualtDropdown">
        
    </select>
</div>
