    			<option>Isaac Presets</option>
    			<option>Jensen</option>
    			<option>Generic</option>
    		</optgroup>
    	</select>
    </div>
    <div id="messagingCore_channels_messageContainer"></div>
    <textarea id="messagingCore_messageTextarea" placeholder="TYPE NEW MESSAGE HERE"></textarea>
    <input type="button" id="messagingCore_sendMessageButton" value="SEND MESSAGE">
