		<input type="button" class="coreButton" id="sensorScansWidget_presetBox_missionSelect_nextButton" value="Next">
	</div>

	<textarea id="sensorScansWidget_responseTextarea"></textarea>

	<!-- responseButtons -->
	<div id="sensorScansWidget_responseButtons">
