<link rel="stylesheet" type="text/css" href="/stationThemes">
<link rel="stylesheet" type="text/css" href="/resource?path=style.css">
<div id="contactInfoBox" class="Container0">
	<div id="contactInfoBox_nameLabel">UNKNOWN CONTACT</div>
	<div id="contactInfoBox_image"></div>
</div>
<div id="contentArea" class="noselect">
	<div id="sensorsArray">
		<canvas id="sensorsArray_Canvas" class="noselect"></canvas>
		<svg width="100%" height="100%" id="mask">
			<defs>
				<clipPath id="myClip">
				<circle cx="50%" cy="50%" r="40%" id="mask_circle"/>
				</clipPath>
			</defs>
		</svg>
	</div>
	<div id="sensorScans" class="container4">
		<div id="processedDataHeader">SENSOR SCANS</div>
		<div id="scanQuerry">
			<input id="scanTextbox" type="text">
			<div id="scanButton" class="Button2 noselect beepOnClick">
				SCAN
			</div>
			<select id="directionDropdown">
				<option value="-1">ALL DIRECTIONS</option>
				<option value="1">1 O'CLOCK</option>
				<option value="2">2 O'CLOCK</option>
				<option value="3">3 O'CLOCK</option>
				<option value="4">4 O'CLOCK</option>
				<option value="5">5 O'CLOCK</option>
				<option value="6">6 O'CLOCK</option>
				<option value="7">7 O'CLOCK</option>
				<option value="8">8 O'CLOCK</option>
				<option value="9">9 O'CLOCK</option>
				<option value="10">10 O'CLOCK</option>
				<option value="11">11 O'CLOCK</option>
				<option value="12">12 O'CLOCK</option>
			</select>
		</div>
		<div id="scanAnswer">
			<div id="scanAnswerTextArea">
			</div>
		</div>
	</div>
	<div id="sensorsArrayViewTypeContainer" class="container4">
		<div id="visibleButton" class="Button2 noselect beepOnClick viewTypeButton verticalAlign selectedButton" style="left:5%;">VISIBLE LIGHT</div>
		<div id="infraredButton" class="Button2 noselect beepOnClick viewTypeButton verticalAlign">INFRARED</div>
	</div>
	<div id="processedData" class="container4">
		<div id="processedDataHeader">PROCESSED DATA</div>
		<div id="increaseFontSize" class="fontSizeAdjustButton">+</div>
		<div id="decreaseFontSize" class="fontSizeAdjustButton" style="right:15%">-</div>
		<div id="processedDataContainer">
		<b style="color:red">WARNING!</b>  THE PROCESSED DATA CANNOT BE COMPILED AT THIS TIME.  PLEASE RELOAD THIS SCREEN.
		</div>
	</div>
</div>

<script src="/threeJS"></script>
<script src="/jquery" type="text/javascript" onload="window.$ = window.jQuery = module.exports;"></script>
<script src="/resource?path=sensorsRender.js" type="text/javascript"></script>
<script src="/resource?path=script.js" type="text/javascript"></script>